{
  "name": "language-tiger",
  "version": "0.5.1",
  "description": "Tiger language support in Atom and Visual Studio Code",
  "keywords": [
    "language",
    "grammar"
  ],
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "repository": "https://github.com/PolariTOON/language-tiger",
  "author": "Tristan LE GODAIS",
  "publisher": "TrisTOON",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0",
    "vscode": "^1.44.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "tiger",
        "extensions": [
          ".tg",
          ".tgr",
          ".tig",
          ".tiger"
        ],
        "configuration": "./settings/language-tiger.json"
      }
    ],
    "grammars": [
      {
        "language": "tiger",
        "scopeName": "source.tiger",
        "path": "./grammars/language-tiger.json"
      }
    ],
    "snippets": [
      {
        "language": "tiger",
        "path": "./snippets/language-tiger.json"
      }
    ]
  }
}
